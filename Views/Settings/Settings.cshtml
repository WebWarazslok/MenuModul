@inherits DotNetNuke.Web.Mvc.Framework.DnnWebViewPage <Company.Modules.MenuModul.Models.Settings>

@using DotNetNuke.Web.Mvc.Helpers

<h2 id="dnnSitePanel-BasicSettings" class="dnnFormSectionHead"><a href="" class="dnnSectionExpanded">@Dnn.LocalizeString("BasicSettings")</a></h2>
<fieldset>
    <div class="dnnFormItem">
        <div class="dnnLabel" style="position: relative;">
            @Html.LabelFor(m => Model.CategoryId, new { @class = "col-md-2 control-label" })
        </div>
        @Html.DropDownList("MovieType")
    </div>
    <div class="dnnFormItem">
        <div class="dnnLabel" style="position: relative;">
            @Html.LabelFor(m => Model.ProductId, new { @class = "col-md-2 control-label" })
        </div>
        @Html.TextBoxFor(m => Model.ProductId)
    </div>
</fieldset>

